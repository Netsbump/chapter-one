			"settings": {
				"foreground": "#6A7680",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.definition",
				"punctuation.separator",
				"meta.brace"
