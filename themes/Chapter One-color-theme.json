	"tokenColors": [
		{
			"name": "Variables",
			"scope": [
				"variable",
				"variable.other.readwrite",
				"variable.other.constant"
			],
			"settings": {
				"foreground": "#1c0810"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#1c0810",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords & Module Keywords",
			"scope": [
			  "keyword.control",
			  "keyword.operator",
			  "keyword.other",
			  //private, public, readonly, static
			  "storage.modifier.ts",
			  //interface, enums, class
			  "storage.type",
			  //function, async
			  "storage.type.function.ts",
			  //import, export, from
			  "keyword.control.import",
			  "keyword.control.export",
			  "keyword.control.from"
			],
			"settings": {
			  "foreground": "#8e1e2c",
			  "fontStyle": "italic"
			}
		},
		{
			"name": "Interface Names",
			"scope": [
				"entity.name.type.interface",
			],
			"settings": {
				"foreground": "#4e625c" ,
			}
		},
		{
			"name": "Type Names",
			"scope": [
				"entity.name.type",
			],
			"settings": {
				"foreground": "#3e6355",
			}
		},
		{
			"name": "Class Names",
			"scope": [
				"entity.name.type.class",
			],
			"settings": {
				"foreground": "#b05361",
			}
		},
		{
			"name": "Methods Class Names",
			"scope": [
    			"meta.class.ts meta.method.declaration.ts entity.name.function.ts"
  			],
			"settings": {
    			"foreground": "#3366cc",
			}
		},
		{
			"name": "Function Names",
			"scope": [
				"entity.name.function",
				"support.function",
			],
			"settings": {
				"foreground": "#7a3e9d",
			}
		},
		{ "name": "Decorators",
          "scope": [
			"meta.decorator.ts meta.function-call.ts entity.name.function.ts",
			"punctuation.decorator",
			"meta.decorator.ts meta.brace.round.ts"
		  ], 
		  "settings": {
			"foreground": "#8B5E13",
			"fontStyle": "italic"
		  }
		},
		{
			"name": "Object Property Keys (left side)",
			"scope": [
			  "meta.object-literal.key",
			  "meta.object-literal.key.ts",
			  "support.type.property-name"
			],
			"settings": {
			  "foreground": "#8e1e2c"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.quoted.single",
				"string.quoted.double"
			],
			"settings": {
				"foreground": "#a34725"
			}
		},
		{ 
			"name": "This",
			"scope": [
				"variable.language.this.ts"
			],
			"settings": {
				"foreground": "#b4752d",
				"fontStyle": "italic"
			}
		},
